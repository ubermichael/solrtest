  <table class="table table-bordered table-condensed table-hover table-striped">
      <thead>
          <tr>
              <th>ID</th>                                       <th>Name</th>
                                                         <th>Street Address</th>
                                                         <th>Start Date</th>
                                                         <th>End Date</th>
                  
          </tr>
      </thead>
      <tbody>
      {% for firm in firms %}
          <tr>
              <td>
                  <a href="{{ path('firm_show', { 'id': firm.id }) }}">
                      {{ firm.id }}
                  </a>
              </td>
              <td>
                  {{ firm.name }}
              </td>
          
              <td>
                  {{ firm.streetAddress|raw }}
              </td>
          
              <td>
                  {{ firm.startDate }}
              </td>
          
              <td>
                  {{ firm.endDate }}
              </td>
          
          </tr>
      {% endfor %}
      </tbody>
  </table>
  